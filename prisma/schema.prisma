datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

type Message{
  title String
  body String
}

type DiaryPage{
  date String
  message Message
}

model User {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  email   String   @unique
  name    String?
  password String?
  dairyPage DiaryPage[]
}

